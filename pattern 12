public static void Print(int n){
	        for(int i=1;i<=n;i++){
	            //no
	            for(int j=1;j<=i;j++){
	                System.out.print(j);
	            }
	            //space
	            for(int j=1;j<=2*n-(2*i);j++){
	                System.out.print(" ");
	            }
	            //no
	            for(int j=i;j>=1;j--){
	                System.out.print(j);
	            }
	            System.out.println();
	        }
	}

  1      1
  12    21
  123  321
  12344321
