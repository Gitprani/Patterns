public static void Print1(int n){
		    for(int i=0;i<n;i++){
		        for(int j=0;j<n;j++){
		            if(i==0||i==n-1||j==0||j==n-1){     // only for edges
		                System.out.print("*");
		            }
		            else{
		                System.out.print(" ");
		            }
		        }
		     System.out.println();
		   }
	}
